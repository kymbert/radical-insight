    "start": "./bin/www",
    "start:dev": "cp .env.development .env && NODE_ENV=development npx nodemon -e js ./bin/www"
  },
  "dependencies": {
    "acorn": "^6.4.1",
    "babel-cli": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "express-winston": "^4.0.3",
    "fibers": "^4.0.1",
    "formik": "^2.1.5",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-rangeslider": "^2.2.0",
    "rebass": "^4.0.7",
    "validator": "^12.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint-plugin-mocha": "^7.0.1",
    "husky": "^2.7.0",
    "jsdoc": "^3.6.3",
    "mocha": "^8.0.1",
    "mongodb-memory-server": "^6.6.1",
    "nodemon": "^1.19.2",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ./bin/lint-all.sh"
    }
  }
