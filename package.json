    "test:prod": "./node_modules/.bin/wdio wdio.prod.conf.js"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "less": "^3.9.0",
    "morgan": "~1.9.0",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "@wdio/cli": "^5.10.4",
    "@wdio/dot-reporter": "^5.9.3",
    "@wdio/local-runner": "^5.10.4",
    "@wdio/mocha-framework": "^5.10.1",
    "@wdio/sync": "^5.10.1",
    "chromedriver": "^75.0.0",
    "geckodriver": "^1.16.2",
    "husky": "^2.4.1",
    "mochawesome-report-generator": "^3.1.5",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "wdio-chromedriver-service": "^5.0.2",
    "wdio-mochawesome-reporter": "^3.1.0",
    "wdio-selenium-standalone-service": "0.0.12"
  },
  "husky": {
