{
  "name": "radical-insight",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "eslint": "./bin/lint-all-report.sh",
    "heroku-postbuild": "cd client && npm i && npm run build",
    "prettify": "pretty-quick",
    "test": "mocha ./test/**/*.spec.js",
    "start": "./bin/www",
    "start:dev": "cp .env.development .env && NODE_ENV=development npx nodemon -e js ./bin/www"
  },
  "dependencies": {
    "acorn": "^7.3.1",
    "babel-cli": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "express-winston": "^4.0.5",
    "fibers": "^5.0.0",
    "formik": "^2.1.5",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.27",
    "morgan": "~1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-rangeslider": "^2.2.0",
    "rebass": "^4.0.7",
    "validator": "^13.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "eslint-plugin-mocha": "^7.0.1",
    "husky": "^4.2.5",
    "jsdoc": "^3.6.5",
    "mocha": "^8.1.0",
    "mongodb-memory-server": "^6.6.3",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ./bin/lint-all.sh"
    }
  }
